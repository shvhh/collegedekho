   <script>
      $(document).ready(function () {
          var collegeSection = document.getElementById("collegeSection").innerHTML;
          var collegeSlag = document.getElementById("collegeSlag").innerHTML;
      
      
      
          $(".collegeInfo a").each(function () {
              console.log($(this).attr("href"));
              console.log("/collegeDetail/" + collegeSlag + "/" + collegeSection);
      
              if (
                  $(this).attr("href") == "/collegeDetail/" + collegeSlag + "/" + collegeSection
              ) {
                  // add active class on his li child//
                  console.log($(this).attr("href"));
      
                  $(this).children("li").addClass("optionactive");
              }
          });
      });
