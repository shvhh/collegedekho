        <p hidden id="sectionLength"> <%=data.section?.length%></p>
        <p hidden id="syllabusLength"> <%=data.syllabus?.length%></p>
        <p hidden id="sallarylength"> <%=data.sallary?.length%></p>

        <footer>

        </footer>
      </div>
