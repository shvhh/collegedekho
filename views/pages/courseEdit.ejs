        <section id="multiple-column-form">
          <div class="row match-height">
            <div class="col-12">
              <div class="card">
                <div class="card-header">

                  <!-- <button type="button" id="btn1" class="btn btn-primary float-start float-lg-end">ADD SUBPARTS</button> -->

               <h4 class="card-title"></h4> 
                </div>
                <div class="card-content">
                  <div class="card-body">
                    <form action="/admin/courseUpdate/<%=data?._id%>" method="post" enctype="multipart/form-data">
                      <div class="row">

                        
                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Name</label>
                            <input type="text" class="form-control" placeholder="Enter Name"value="<%=data?.name%>" name="name" data-parsley-required="true" required />
                          </div>
                        </div>

                    


                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="company-column" class="form-label">Image</label>
                            <input type="hidden" id="imagesdata" name="image" />

                            <input type="file" class="form-control"id="imagetype"onchange="previewImageHere(event)" placeholder="Enter Url"  name="image" data-parsley-required="true"/>
                          </div>
                         <div class="row">
                            <div class="col-md-2">
                              <div class="imgsecccc">
                                <img src="<%=data?.image%>" id="img" alt="alt" />
                             
                              </div>
                            </div>
                        </div> 

        
                        </div>

                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Alt For Image </label>
                            <input type="text" class="form-control" placeholder="Enter Alt  For Image " value="<%=data?.imageAlt%>" name="imageAlt" data-parsley-required="true" required />
                          </div>
                        </div>
                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Average Duration (In Month)</label>
                            <input type="text" class="form-control" placeholder="Enter Average Duration" value="<%=data?.averageDuration%>" name="averageDuration" data-parsley-required="true" required />
                          </div>
                        </div>


                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Fees </label>
                            <input type="text" class="form-control" placeholder="Enter Average Duration" value="<%=fees%>" name="fees" data-parsley-required="true" required />
                          </div>
                        </div>

                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label"
                              >Mode</label
                            >
                        <select class="form-select" aria-label="Default select example" id="data" name="mode" required>
                     
                          <option value="">Open this select menu</option>
       
                          <%mode?.forEach(function(mode){ %>
                          <option value="<%=mode%>"  <%= (`${mode}` === `${data.mode}`)? 'selected' : '' %>><%=mode%></option>
                    


                          <% }) %>
                          


               



                         
                         
                        

          
                        </select>
                        </div></div>

                         
                 
                            <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Short Description</label>
                            <input type="text" class="form-control" placeholder="Enter Short Description" value="<%=data.shortDescription%>" name="shortDescription" data-parsley-required="true" required />
                          </div>
                        </div>

                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label"
                              >Category</label
                            >
                        <select class="form-select" aria-label="Default select example" id="cars" name="courseCategoryId" >
                     
                          <option value="">Open this select menu</option>
          <% category?.forEach(function(category){ %>
                          <option value="<%=category?._id%>" <%= (`${category?._id}` === `${data?.courseCategoryId}`)? 'selected' : '' %>> <%=category?.category%></option>
   

          <% }) %>
                        </select>
                        </div></div>


                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Keyword </label>
                            <input type="text" class="form-control" placeholder="Enter Keyword " value="<%=data?.metaKeyword%>" name="metaKeyword" data-parsley-required="true" required />
                          </div>
                        </div>

                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label"> Description</label>
                            <input type="text" class="form-control" placeholder="Enter  Description"value="<%=data?.metaDescription%>" name="metaDescription" data-parsley-required="true" required />
                          </div>
                        </div>

                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Title </label>
                            <input type="text" class="form-control" placeholder="Enter Title" value="<%=data?.metaTitle%>" name="metaTitle" data-parsley-required="true" required />
                          </div>
                        </div>

                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Colonial </label>
                            <input type="text" class="form-control" placeholder="Enter Canonical" value="<%=data?.canonical%>" name="canonical" data-parsley-required="true" required />
                          </div>
                        </div>
                      


                        
                        <div class="col-md-6 col-12">
                          <div class="form-group mandatory">
                            <label for="first-name-column" class="form-label">Matched Keyword </label>
                            <input type="text" class="form-control" placeholder="Enter Matched Keyword" value="<%=data?.matchKeyword%>" name="matchKeyword" data-parsley-required="true" required />
                          </div>
                        </div>
                      

    
     




    


<p id="textbox"></p>

                     
<!-- syllabus// -->




<%if(data?.isTop==true){%>
  <div>
    <input type="checkbox" id="vehicle1"checked name="isTop" >
    <label for="popular">Popular Course</label><br>
  </div>
  <% }else{ %>
    <div>
    <input type="checkbox" id="vehicle2" name="isTop" >
    <label for="popular">Popular Course</label><br>
  </div>
  <% } %>








