    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.sumoselect/3.4.9/jquery.sumoselect.min.js" integrity="sha512-+Ea4TZ8vBWO588N7H6YOySCtkjerpyiLnV7bgqwrQF+vqR8+q/InGK9WDZx5d6VtdGRoV6uLd5Dwz2vE7EL3oQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>
// get image form imagetype input and set it to img src
function previewImageHere(event) {
  console.log(event.target.files[0]);
      var input = event.target;

      var data = new FormData();
data.append("file", input.files[0]);
 
var xhr = new XMLHttpRequest();
// xhr.withCredentials = true;

xhr.addEventListener("readystatechange", function() {
  if(this.readyState === 4) {
    if(input.id == 'imagetype'){
            // var preview = document.getElementsByClassName('imageArea');

            document.getElementById('imagesdata').value = this.responseText;
          // preview.innerHTML = '<img  src="' + this.responseText + '"/>';
          $('.imgsecccc').html('<img  src="' + this.responseText + '"/>');
    }
  }
});

xhr.open("POST", "/admin/file");
xhr.send(data);



    }

  





   
      $(document).ready(function() {
        let count = document.getElementById("sectionLength").innerHTML
console.log(count)
let counter = count - 1
        $("#btn1").click(function() {

          const id = 'multiAdd' + Math.floor(Math.random() * 10000000000000000000);
          counter++
          $("#textbox").append('<div class="col-md-6 col-12" id="btn3' + id + '" ><div class="form-group mandatory"><label for="first-name-column" class="form-label"> Section Heading</label><input type="text" class="form-control" placeholder="First Secton Heading" name="section[' + counter + '][heading]" data-parsley-required="true" /></div></div>')
          $("#textbox").append('<section class="section"  id="btn389' + id + '"  ><div class="row"><div class="col-12"><div class="card"><div class="card-header"><!-- <h4 class="card-title">Default Editor</h4> --></div><h5 class="editorMargin">Section Description</h5><div class="card-body"><div></div><textarea id="' + id + '" name="section[' + counter + '][description]"></textarea></div></div></div></div></section>');
          $('#textbox').append('<button type="button" class="btn btn-danger float-start float-lg-end me-1 mb-60" id=' + id + ' onclick="deleteDiv(this.id)">Delete</button>');
          $('#' + id).summernote({ 
             tabsize: 2,
  height: 120,
});
          
         
        });
      });



    


      // delete text area and editor//
      function deleteDiv(id) {
       

        $('#btn3' + id).remove();
        $('#btn389' + id).remove();
        $('#' + id).remove();

      }
    </script>



<!-- script for syllabus/// -->


<script>


      $(document).ready(function() {
        let count = document.getElementById("syllabusLength").innerHTML
console.log(count)
let syllabus = count - 1
        $("#btn2").click(function() {

          const id = 'multiAdd' + Math.floor(Math.random() * 100000000000000000000);
          syllabus++
          $("#syllaboxBox").append('<div class="col-md-6 col-12" id="btn3' + id + '" ><div class="form-group mandatory"><label for="first-name-column" class="form-label"> Syllabus Heading</label><input type="text" class="form-control" placeholder="First Syllabus Heading" name="syllabus[' + syllabus + '][heading]" data-parsley-required="true" /></div></div>')
          $("#syllaboxBox").append('<section class="section"  id="btn389' + id + '"  ><div class="row"><div class="col-12"><div class="card"><div class="card-header"><!-- <h4 class="card-title">Default Editor</h4> --></div><h5 class="editorMargin">Syllabus Description</h5><div class="card-body"><div></div><textarea id="' + id + '" name="syllabus[' + syllabus + '][description]"></textarea></div></div></div></div></section>');
          $('#syllaboxBox').append('<button type="button" class="btn btn-danger float-start float-lg-end me-1 mb-60" id=' + id + ' onclick="deleteDiv(this.id)">Delete</button>');
          $('#' + id).summernote({ 
             tabsize: 2,
  height: 120,
});
          counter++;
          
        });
      });



    


      // delete text area and editor//
      function deleteDiv(id) {
     

        $('#btn3' + id).remove();
        $('#btn389' + id).remove();
        $('#' + id).remove();

      }
</script>


<!-- script for sallary/ -->

<script>


        $(document).ready(function() {
          let count = document.getElementById("sallarylength").innerHTML
console.log(count)
let salary = count - 1
          $("#btn3").click(function() {
  
            const id = 'multiAdd' + Math.floor(Math.random() * 1000000000000000000000);
            salary++
            $("#sallaryBox").append('<div class="col-md-6 col-12" id="btn3' + id + '" ><div class="form-group mandatory"><label for="first-name-column" class="form-label"> Sallary Heading</label><input type="text" class="form-control" placeholder= Sallary Heading" name="sallary[' + salary + '][heading]" data-parsley-required="true" /></div></div>')
            $("#sallaryBox").append('<section class="section"  id="btn389' + id + '"  ><div class="row"><div class="col-12"><div class="card"><div class="card-header"><!-- <h4 class="card-title">Default Editor</h4> --></div><h5 class="editorMargin">Sallary Description</h5><div class="card-body"><div></div><textarea id="' + id + '" name="sallary[' + salary + '][description]"></textarea></div></div></div></div></section>');
            $('#sallaryBox').append('<button type="button" class="btn btn-danger float-start float-lg-end me-1 mb-60" id=' + id + ' onclick="deleteDiv(this.id)">Delete</button>');
            $('#' + id).summernote({ 
               tabsize: 2,
    height: 120,
  });
            counter++;
   
          });
        });
  
  
  
      
  
  
        // delete text area and editor//
        function deleteDiv(id) {
        
  
          $('#btn3' + id).remove();
          $('#btn389' + id).remove();
          $('#' + id).remove();
  
        }
  </script>

</body>

